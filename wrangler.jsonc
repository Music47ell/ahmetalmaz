{

  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ahmetalmaz",
  "main": "./dist/_worker.js/index.js",
  // Update to today's date
  "compatibility_date": "2025-09-29",
	"compatibility_flags": [
    "nodejs_compat",
		"nodejs_compat_populate_process_env"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
   "routes": [
    {
      "pattern": "ahmetalmaz.com",
      "custom_domain": true
    }
  ],
}